<?xml version="1.0"?>
<ML-CONFIG>
   <VERBOSITY level="2"/>
   <SURROUND value="false"/>
   <FILTERING ratio="0.0" dis="near"/>

   <PARAMETER name="thresholdProbabilityClassification" value="0.5"/>

   <multiClassification2Binary method="one-vs-others"/>

   <!-- Evaluation : how to split the corpus into test and learn? -->
   <EVALUATION method="kfold" runs="10" />

   <!-- ENGINES 
   <ENGINE nickname="SVM" implementationName="SVMLibSvmJava" 
         options=" -c 0.7 -t 0 -m 100 -tau 0.6"/>
   <ENGINE nickname="PAUM" implementationName="PAUM"  
    options="-p 50 -n 5 -optB 0.3"/> 
   <ENGINE nickname="C45" implementationName="C4.5Weka"/>
   <ENGINE nickname="KNN" implementationName="KNNWeka"  options=" â€“k 1"/>
   <ENGINE nickname="NBW" implementationName="NaiveBayesWeka"/>-->

   <ENGINE nickname="PAUM" implementationName="PAUM"  
    options="-p 50 -n 5 -optB 0.3"/>

   <DATASET>
     <INSTANCE-TYPE>RelationInstance</INSTANCE-TYPE>
     <INSTANCE-ARG1>Domain-id</INSTANCE-ARG1>
     <INSTANCE-ARG2>Range-id</INSTANCE-ARG2>

     <FEATURES-ARG1>
       <ARG>
        <NAME>ARG1</NAME>
        <SEMTYPE>NOMINAL</SEMTYPE>
        <TYPE>RelationInstance</TYPE>
        <FEATURE>Domain-id</FEATURE>
       </ARG>

       <ATTRIBUTE>
         <NAME>ClassC</NAME>
         <SEMTYPE>NOMINAL</SEMTYPE>
         <TYPE>Lookup</TYPE>
         <FEATURE>class</FEATURE>
         <RANGE from="-5" to="5" />
       </ATTRIBUTE>
       <ATTRIBUTE>  
         <NAME>ClassG</NAME>  
         <SEMTYPE>NOMINAL</SEMTYPE>  
         <TYPE>GeneOntology</TYPE>  
         <FEATURE>majorType</FEATURE>
         <RANGE from="-5" to="5"/>  
       </ATTRIBUTE>
       <ATTRIBUTE>
         <NAME>POSC</NAME>
         <SEMTYPE>NOMINAL</SEMTYPE>
         <TYPE>Token</TYPE>
         <FEATURE>category</FEATURE>
         <RANGE from="-5" to="5" />
       </ATTRIBUTE>
      </FEATURES-ARG1>

       <FEATURES-ARG2>
        <ARG>
          <NAME>ARG2</NAME>
          <SEMTYPE>NOMINAL</SEMTYPE>
          <TYPE>RelationInstance</TYPE>
          <FEATURE>Range-id</FEATURE>
        </ARG>

        <ATTRIBUTE>
         <NAME>ClassC</NAME>
         <SEMTYPE>NOMINAL</SEMTYPE>
         <TYPE>Lookup</TYPE>
         <FEATURE>class</FEATURE>
         <RANGE from="-5" to="5" />
       </ATTRIBUTE>
       <ATTRIBUTE>  
         <NAME>ClassG</NAME>  
         <SEMTYPE>NOMINAL</SEMTYPE>  
         <TYPE>GeneOntology</TYPE>  
         <FEATURE>majorType</FEATURE>
         <RANGE from="-5" to="5"/>  
       </ATTRIBUTE>
         <NAME>POSC</NAME>
         <SEMTYPE>NOMINAL</SEMTYPE>
         <TYPE>Token</TYPE>
         <FEATURE>category</FEATURE>
         <RANGE from="-5" to="5" />
       </ATTRIBUTE>
      </FEATURES-ARG2>

     <ATTRIBUTE_REL>
        <NAME>distance</NAME>
        <SEMTYPE>NOMINAL</SEMTYPE>
        <TYPE>RelationInstance</TYPE>
        <ARG1>Domain-id</ARG1>
        <ARG2>Range-id</ARG2>
        <FEATURE>distance</FEATURE>
    </ATTRIBUTE_REL>

     <ATTRIBUTE_REL>
        <NAME>order</NAME>
        <SEMTYPE>NOMINAL</SEMTYPE>
        <TYPE>RelationInstance</TYPE>
        <ARG1>Domain-id</ARG1>
        <ARG2>Range-id</ARG2>
        <FEATURE>order</FEATURE>
    </ATTRIBUTE_REL>

     <!--<ATTRIBUTE_REL>
        <NAME>verbRelation</NAME>
        <SEMTYPE>NOMINAL</SEMTYPE>
        <TYPE>RelationInstance</TYPE>
        <ARG1>Domain-id</ARG1>
        <ARG2>Range-id</ARG2>
        <FEATURE>rel-type</FEATURE>
    </ATTRIBUTE_REL>-->

    <ATTRIBUTE_REL>
        <NAME>poslist</NAME>
        <SEMTYPE>NOMINAL</SEMTYPE>
        <TYPE>RelationInstance</TYPE>
        <ARG1>Domain-id</ARG1>
        <ARG2>Range-id</ARG2>
        <FEATURE>poslist</FEATURE>
    </ATTRIBUTE_REL>

    <ATTRIBUTE_REL>
      <NAME>Class</NAME>
      <SEMTYPE>NOMINAL</SEMTYPE>
      <TYPE>RelationClass</TYPE>
      <ARG1>Domain-id</ARG1>
      <ARG2>Range-id</ARG2>
      <FEATURE>rel-type</FEATURE>
      <CLASS/>
    </ATTRIBUTE_REL>
 </DATASET>

</ML-CONFIG>

